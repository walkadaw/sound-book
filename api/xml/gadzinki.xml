            <w:b/>
        </w:rPr>
    </w:pPr>
    <w:r w:rsidRPr="00B524DE">
        <w:rPr>
            <w:b/>
        </w:rPr>
        <w:t>К.:</w:t>
    </w:r>
    <w:r w:rsidRPr="00B524DE">
        <w:t xml:space="preserve"> Тваё імя, о Марыя, як алей духмяны.</w:t>
    </w:r>
    <w:r>
        <w:br/>
    </w:r>
    <w:r w:rsidRPr="00FF6F05">
        <w:rPr>
            <w:b/>
        </w:rPr>
        <w:t>Усе.: А Твае слугі моцна Цябе ўзлюбілі.</w:t>
    </w:r>
</w:p>
