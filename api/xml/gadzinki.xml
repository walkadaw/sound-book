        <w:pStyle w:val="af"/>
        <w:spacing w:before="0"/>
        <w:rPr>
            <w:b/>
        </w:rPr>
    </w:pPr>
    <w:r w:rsidRPr="00B524DE">
        <w:rPr>
            <w:b/>
        </w:rPr>
        <w:t>К.:</w:t>
    </w:r>
    <w:r w:rsidRPr="00B524DE">
        <w:t xml:space="preserve"> А душы верных памерлых праз Божую міласэрнасць няхай спачываюць у супакоі вечным.</w:t>
    </w:r>
    <w:r>
        <w:br/>
    </w:r>
    <w:r w:rsidRPr="00301891">
        <w:rPr>
            <w:b/>
        </w:rPr>
        <w:t>Усе: Амэн.</w:t>
    </w:r>
</w:p>
