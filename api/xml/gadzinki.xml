<w:p w:rsidR="00755FC1" w:rsidRPr="001F2590" w:rsidRDefault="00755FC1" w:rsidP="001F2590">
    <w:pPr>
        <w:pStyle w:val="a"/>
    </w:pPr>
    <w:r w:rsidRPr="001F2590">
        <w:t xml:space="preserve">Шляхам вядзі нас збаўленным, памажы ў патрэбе </w:t>
    </w:r>
    <w:r w:rsidRPr="001F2590">
        <w:br/>
        <w:t>не пакідай пры смерці, дай нам быць у небе.</w:t>
    </w:r>
</w:p>
