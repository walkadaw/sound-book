        <w:pStyle w:val="af"/>
    </w:pPr>
    <w:r w:rsidRPr="00B524DE">
        <w:rPr>
            <w:b/>
        </w:rPr>
        <w:lastRenderedPageBreak/>
        <w:t>К.:</w:t>
    </w:r>
    <w:r w:rsidRPr="00B524DE">
        <w:t xml:space="preserve"> Я ўчыніла, каб на небе ўзыходзіла святло няспынна.</w:t>
    </w:r>
</w:p>
<w:p w:rsidR="00755FC1" w:rsidRPr="00BD0AA7" w:rsidRDefault="00755FC1" w:rsidP="00755FC1">
    <w:pPr>
        <w:pStyle w:val="af"/>
        <w:rPr>
            <w:b/>
        </w:rPr>
    </w:pPr>
    <w:r w:rsidRPr="00BD0AA7">
        <w:rPr>
            <w:b/>
        </w:rPr>
        <w:t>Усе: І быццам імгліца ўсю зямлю агарнула.</w:t>
    </w:r>
</w:p>
<w:p w:rsidR="00755FC1" w:rsidRPr="00B524DE" w:rsidRDefault="00755FC1" w:rsidP="00755FC1">
    <w:pPr>
        <w:pStyle w:val="af"/>
    </w:pPr>
    <w:r w:rsidRPr="00B524DE">
        <w:rPr>
            <w:b/>
        </w:rPr>
        <w:t>К.:</w:t>
    </w:r>
    <w:r w:rsidRPr="00B524DE">
        <w:t xml:space="preserve"> Пані, выслухай малітвы нашы.</w:t>
    </w:r>
</w:p>
<w:p w:rsidR="00755FC1" w:rsidRPr="00BD0AA7" w:rsidRDefault="00755FC1" w:rsidP="00755FC1">
