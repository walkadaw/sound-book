            <w:b/>
        </w:rPr>
        <w:t>К.:</w:t>
    </w:r>
    <w:r w:rsidRPr="00B524DE">
        <w:t xml:space="preserve"> Праслаўляйма Пана.</w:t>
    </w:r>
    <w:r>
        <w:br/>
    </w:r>
    <w:r w:rsidRPr="00301891">
        <w:rPr>
            <w:b/>
        </w:rPr>
        <w:t>Усе: Богу хвала.</w:t>
    </w:r>
</w:p>
<w:p w:rsidR="00755FC1" w:rsidRPr="00301891" w:rsidRDefault="00755FC1" w:rsidP="001F2590">
